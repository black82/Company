    var currentName = "Alix Ben Hur";
    var syllables = ["a","acyn","ar","as","be","bel","ben","ber","bert","bet","bi","bir","bor","bra","bus","ca","cas","ce","cel","cus","cy","cyl","da","dam","de","di","do","don","dor","dras","dre","dri","dus","e","em","er","es","et","fa","fan","fran","fre","fus","fus","ga","gab","gan","ge","gen","ger","gi","git","gus","ha","ham","har","he","hel","her","him","ho","hor","hu","hus","i","im","in","ir","is","it","jus","ka","kat","ke","ken","ko","kus","la","lan","land","le","les","li","lin","lo","lon","lor","lu","lus","ma","mad","man","mar","mas","mat","me","mer","mu","mus","na","nah","nas","ni","nik","nim","nis","no","nor","nos","nus","o","om","on","or","os","ot","pe","per","pres","pus","qus","ra","rah","re","ret","ri","rik","rin","ris","ro","rob","rod","ron","rus","sa","san","scar","so","son","sper","stan","ste","su","ta","tan","te","ted","ter","tes","to","tom","ton","tond","tu","tur","tus","u","um","un","ur","us","ut","va","vak","van","var","ve","ven","ver","vid","vin","vus","wus","xi","xus","zel"];

    //returns a random number between min (included) and max (excluded)
    function getRndInteger(min, max) {
        return Math.floor(Math.random() * (max - min) ) + min;
    }
    function CreateNewName()
    {
        //Creates a first name with 2-3 syllables
        var firstName = "";
        var numberOfSyllablesInFirstName = getRndInteger(1, 3);
        for (var i = 0; i < numberOfSyllablesInFirstName; i++)
        {
            firstName += syllables[getRndInteger(0, syllables.length)];
        }
        var firstNameLetter = "";
        firstNameLetter = firstName.substring(0,1);
        firstName = firstName.substring(1);
        firstNameLetter = firstNameLetter.toUpperCase();
        firstName = firstNameLetter + firstName;

        //Creates a middle name with 1-2 syllables
        var middleName = "";
        var numberOfSyllablesInMiddleName= getRndInteger(2, 3);
        for (var j = 0; j < numberOfSyllablesInMiddleName; j++)
        {
            middleName += syllables[getRndInteger(0, syllables.length)];
        }
        var middleNameLetter = "";
        middleNameLetter = middleName.substring(0,1);
        middleName = middleName.substring(1);
        middleNameLetter = middleNameLetter.toUpperCase();
        middleName = middleNameLetter + middleName;

        //Creates a last name with 1-2 syllables
        var lastName = "";
        var numberOfSyllablesInLastName = getRndInteger(2, 4);
        for (var j = 0; j < numberOfSyllablesInLastName; j++)
        {
            lastName += syllables[getRndInteger(0, syllables.length)];
        }
        var lastNameLetter = "";
        lastNameLetter = lastName.substring(0,1);
        lastName = lastName.substring(1);
        lastNameLetter = lastNameLetter.toUpperCase();
        lastName = lastNameLetter + lastName;

        //assembles the newly-created name
        currentName = firstName + " " + middleName + " " + lastName ;

         $("#newName").val(currentName);
         $("#newName").focus();
    }


      $(document).ready(function () {
        $("#generateName").click(function() { CreateNewName(); });
      });
    </script>

