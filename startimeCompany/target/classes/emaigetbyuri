          $(document).ready(function() {
            var right = $('.ads-sidebar');
            var stop = $('.cp-details-header').offset().top;
            var win_scroll = $(window).scrollTop();
            var contentStop = $('#main').offset().top + $('#main').height();
            //console.log(contentStop);
            getscroll();

            $(window).on('scroll', function () {
              getscroll();
            });

            function getscroll() {
              var win_scroll = $(window).scrollTop();
              //console.log(win_scroll);
              if (win_scroll > contentStop) {
                right.removeClass('sticky');
                right.addClass('content-bottom');
              } else if (win_scroll > stop) {
                right.addClass('sticky');
                right.removeClass('content-bottom');
              } else {
                right.removeClass('sticky');
              }


            }
          });

      </script><script type="text/javascript">

        $(document).ready(function() {
          $(window).scroll(function () {
            var scrollTop = $(this).scrollTop();
            if (scrollTop >= $(window).height()) {
              $("#to-top > a.btn-floating").removeClass("btn-floating-hidden");
            } else {
              $("#to-top > a.btn-floating").addClass("btn-floating-hidden");
            }
          });
        });

