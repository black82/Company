            $(document).ready(function () {
                $(function () {
                    // clear value of hidden city url field as soon as city form field gets focus.
                    $("#search-where").focusin(function () {
                      $("#search-where-hidden").val("");
                      $("#search-where-zip").val("");
                    });
                    $("#search-where").autocomplete({
                        minLength: 2,
                        source: function (request, response) {
                            $.ajax({
                                type: "POST",
                                url: '/webservices/CityAutoSuggest.asmx/SOLRCitySuggest',
                                contentType: "application/json; charset=utf-8;",
                                data: JSON.stringify({ limit: 10, input: $("#search-where").val() , isFiliale: false }),
                                success: function (data) {
                                    response($.map(data.d, function(v,i){
                                      if (v.language!=null){
                                         return {
                                            label: "///" + v.words + ", " + v.place + ", "+ v.country.toUpperCase(),
                                            value: "///" + v.words
                                        };
                                      }
                                      else
                                      {
                                        return {
                                            label: (v.geo_dis_zip!='#' ? v.geo_dis_zip + ", " : "") + v.geo_city + (v.geo_region_display ? ", " + v.geo_region_display : ""),
                                            zip: (v.geo_dis_zip!='#' ? v.geo_dis_zip : ""),
                                            url: v.geo_url,
                                            value: v.geo_city
                                        };
                                      }
                                    }));
                                }
                            });
                        },
                        autoFocus: true,
                        select: function( event, ui ) {
                          if (ui.item)
                          {
                            $("#search-where").val(ui.item.value);
                            $("#search-where-hidden").val(ui.item.url);
                            $("#search-where-zip").val(ui.item.zip);
                          }
                        }
                    });

                    // clear value of hidden city url field as soon as city form field gets focus.
                    $("#xs-search-where").focusin(function () {
                      $("#xs-search-where-hidden").val("");
                      $("#xs-search-where-zip").val("");
                    });
                    $("#xs-search-where").autocomplete({
                        minLength: 2,
                        source: function (request, response) {
                            $.ajax({
                                type: "POST",
                                url: '/webservices/CityAutoSuggest.asmx/SOLRCitySuggest',
                                contentType: "application/json; charset=utf-8;",
                                data: JSON.stringify({ limit: 5, input: $("#xs-search-where").val() }),
                                success: function (data) {
                                response($.map(data.d, function(v,i){
                                      if (v.language!=null){
                                         return {
                                            label: "///" + v.words + ", " + v.place + ", "+ v.country.toUpperCase(),
                                            value: "///" + v.words
                                        };
                                      }
                                      else
                                      {
                                        return {
                                            label: (v.geo_dis_zip!='#' ? v.geo_dis_zip + ", " : "") + v.geo_city + (v.geo_region_display ? ", " + v.geo_region_display : ""),
                                            zip: (v.geo_dis_zip!='#' ? v.geo_dis_zip : ""),
                                            url: v.geo_url,
                                            value: v.geo_city
                                        };
                                      }
                                    }));
                                }
                            });
                        },
                        autoFocus: true,
                        select: function( event, ui ) {
                          if (ui.item)
                          {
                            $("#xs-search-where").val(ui.item.value);
                            $("#xs-search-where-hidden").val(ui.item.url);
                            $("#xs-search-where-zip").val(ui.item.zip);
                          }
                        }
                    });
                });
            });
